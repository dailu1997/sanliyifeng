page {
  background-color: #f4f4f4;
}
.body {
  width: 710rpx;
  margin: 20rpx auto;
}
.none {
  display: none;
}
.right {
  float: right;
  overflow: hidden;
}
.top image {
  width: 28rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.top text {
  font-size: 24rpx;
}
.top {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
  justify-content: space-between;
}
.zuo {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.li {
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  background-color: #fff;
}


.ke image {
  margin-top: 15rpx;
  margin-right: 10rpx;
  line-height: 60rpx;
  float: left;
   width: 106rpx;
  height: 100rpx;
  border-radius:50%; 
}
.sm {
  font-size: 18rpx;
  color: #a7a7a7;
}
.in { 
  float: left;
  font-size: 24rpx;
}
.in text {
  display: block;
  margin-top: 7rpx;
}
.shouquan {
  font-size: 22rpx;
  color: #fff;
  border: 1px solid #cecdcb;
  border-radius: 10rpx;
  background-color: #dd847f;
  padding: 12rpx 30rpx;
  box-sizing: border-box;
}
.xianhao {
  padding: 10rpx 15rpx;
  font-size: 22rpx;
  background-color: #dca97e;
  color: #fff;
  border-radius: 10rpx;
  text-align: center;
  margin-top: 10rpx;
}
.li {
  overflow: hidden;
  width: 710rpx;
  margin: 0 auto ;
  padding: 50rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.li .tuoke {
  font-size: 24rpx;
  color: #666;
}
.ke {
  overflow: hidden;
  width: 710rpx;
  box-sizing: border-box;
  background-color: #fff;
  padding: 20rpx;
  border-bottom: 1px solid #eee;
  border-radius: 10rpx;
}
.ke image {
  margin-top: 15rpx;
  margin-right: 10rpx;
  line-height: 60rpx;
  float: left;
  width: 85rpx;
