radio{
    width: 38rpx;
    font-size: 45rpx;
    margin-left: 20rpx;
}
 
radio .wx-radio-input{
    border-radius: 50%; 
    width: 50rpx;
    height: 50rpx;
}

radio .wx-radio-input.wx-radio-input-checked{
   border: none;
}
 .radio-group {
   float: right;
 }
/* 选中后的样式 （可根据设计稿需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked::before{
    border-radius: 50%;
    width: 76rpx; 
    height: 76rpx; 
    line-height: 76rpx;
    text-align: center;
    font-size: 42rpx; /* 对勾大小 26rpx */
    color:#fff; /* 对勾颜色 */
    background:  rgb(223,146,130);
    border-color: rgb(223,146,130);
}
checkbox .wx-checkbox-input {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
    border-radius: 50%;
    width: 64rpx; 
    height: 64rpx; 
    line-height: 64rpx;
    text-align: center;
    font-size: 42rpx; /* 对勾大小 26rpx */
    color:#fff; /* 对勾颜色 */
    background:  #dca97e;
    border-color:  #dca97e;
}