.footer {
  display: flex;
  align-items: center;
  position: relative;
 line-height: 80rpx;
 font-size:34rpx;
 border-top: 1px solid #ccc;
}
.footer::before{
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  height: 1rpx;
  border-top: 1rpx solid #d5d5d6;
  color: #d5d5d6;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5)
}
.btn {
  display: block;
  -webkit-box-flex: 1;
  flex: 1;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  position: relative;
}
.footer .btn:nth-of-type(1){

  color: #353535;
}
.footer .btn:nth-of-type(2){
  color: #f46c59;
  border-left: 1px solid #ccc;
}
.footer .btn:nth-of-type(2)::after{
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 1rpx;
  border-left:1rpx solid #d5d5d6;
 -webkit-transform-origin: 0 0;
 transform-origin: 0 0;
 -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  color: #d5d5d6;
}
