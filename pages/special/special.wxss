@import "../../wxParse/wxParse.wxss";

.swiper-tab {
    text-align: center;
    line-height: 80rpx;
}

.swiper-tab-list {
    font-size: 30rpx;
    display: inline-block;
    width: 50%;
    color: #777777;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.on {
    color: #da7c0c;
    border-bottom: 5rpx solid #da7c0c;
}

.swiper-box {
    display: block;
    width: 98%;
}